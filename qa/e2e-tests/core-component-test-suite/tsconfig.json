{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": "./",
    "noEmit": true,
    "paths": {
      "@fixtures/*": ["fixtures/*"],
      "@setup": ["./test-setup.ts"],
      "@pages/*": ["pages/*"]
    }
  }
}
