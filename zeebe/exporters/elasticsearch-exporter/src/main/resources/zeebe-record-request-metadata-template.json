{
  "index_patterns": [
    "zeebe-record_request-metadata_*"
  ],
  "composed_of": ["zeebe-record"],
  "priority": 20,
  "version": 1,
  "template": {
    "settings": {
      "number_of_shards": 1,
      "number_of_replicas": 0,
      "index.queries.cache.enabled": false
    },
    "aliases": {
      "zeebe-record-request-metadata": {}
    },
    "mappings": {
      "properties": {
        "value": {
          "dynamic": "strict",
          "properties": {
            "scopeKey": {
              "type": "long"
            },
            "valueType": {
              "type": "keyword"
            },
            "intent": {
              "type": "keyword"
            },
            "requestId": {
              "type": "long"
            },
            "requestStreamId": {
              "type": "integer"
            },
            "operationReference": {
              "type": "long"
            }
          }
        }
      }
    }
  }
}
