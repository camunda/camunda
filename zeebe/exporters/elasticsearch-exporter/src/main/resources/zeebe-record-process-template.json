{
  "index_patterns": [
    "zeebe-record_process_*"
  ],
  "composed_of": ["zeebe-record"],
  "priority": 20,
  "version": 1,
  "template": {
    "settings": {
      "number_of_shards": 1,
      "number_of_replicas": 0,
      "index.queries.cache.enabled": false
    },
    "aliases": {
      "zeebe-record-process": {}
    },
    "mappings": {
      "properties": {
        "value": {
          "dynamic": "strict",
          "properties": {
            "bpmnProcessId": {
              "type": "keyword"
            },
            "version": {
              "type": "long"
            },
            "processDefinitionKey": {
              "type": "long"
            },
            "resourceName": {
              "type": "text"
            },
            "resource": {
              "enabled": false
            },
            "checksum": {
              "enabled": false
            },
            "duplicate": {
              "type": "boolean"
            },
            "tenantId": {
              "type": "keyword"
            },
            "deploymentKey": {
              "type": "long"
            }
          }
        }
      }
    }
  }
}
