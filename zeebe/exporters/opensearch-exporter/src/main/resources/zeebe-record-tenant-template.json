{
  "index_patterns": [
    "zeebe-record_tenant_*"
  ],
  "composed_of": [
    "zeebe-record"
  ],
  "priority": 20,
  "version": 1,
  "template": {
    "settings": {
      "number_of_shards": 1,
      "number_of_replicas": 0,
      "index.queries.cache.enabled": false
    },
    "aliases": {
      "zeebe-record-tenant": {}
    },
    "mappings": {
      "properties": {
        "value": {
          "dynamic": "strict",
          "properties": {
            "entityId": {
              "type": "keyword"
            },
            "tenantKey": {
              "type": "long"
            },
            "tenantId": {
              "type": "keyword"
            },
            "name": {
              "type": "keyword"
            },
            "description": {
              "type": "text"
            },
            "entityType": {
              "type": "keyword"
            }
          }
        }
      }
    }
  }
}
