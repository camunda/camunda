{
  "index_patterns": [
    "zeebe-record_process-instance-creation_*"
  ],
  "composed_of": ["zeebe-record"],
  "priority": 20,
  "version": 1,
  "template": {
    "settings": {
      "number_of_shards": 1,
      "number_of_replicas": 0,
      "index.queries.cache.enabled": false
    },
    "aliases": {
      "zeebe-record-process-instance-creation": {}
    },
    "mappings": {
      "properties": {
        "value": {
          "dynamic": "strict",
          "properties": {
            "bpmnProcessId": {
              "type": "keyword"
            },
            "version": {
              "type": "integer"
            },
            "processDefinitionKey": {
              "type": "long"
            },
            "processInstanceKey": {
              "type": "long"
            },
            "variables": {
              "enabled": false
            },
            "startInstructions": {
              "properties": {
                "elementId": {
                  "type": "keyword"
                }
              }
            },
            "runtimeInstructions": {
              "properties": {
                "type": {
                  "type": "keyword"
                },
                "afterElementId": {
                  "type": "keyword"
                }
              }
            },
            "tenantId": {
              "type": "keyword"
            },
            "tags": {
              "type": "keyword"
            }
          }
        }
      }
    }
  }
}
