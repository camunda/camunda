{
  "index_patterns": [
    "zeebe-record_process-instance-migration_*"
  ],
  "composed_of": ["zeebe-record"],
  "priority": 20,
  "version": 1,
  "template": {
    "settings": {
      "number_of_shards": 1,
      "number_of_replicas": 0,
      "index.queries.cache.enabled": false
    },
    "aliases": {
      "zeebe-record-process-instance-migration": {}
    },
    "mappings": {
      "properties": {
        "value": {
          "dynamic": "strict",
          "properties": {
            "processInstanceKey": {
              "type": "long"
            },
            "processDefinitionKey": {
              "type": "long"
            },
            "targetProcessDefinitionKey": {
              "type": "long"
            },
            "mappingInstructions": {
              "properties": {
                "sourceElementId": {
                  "type": "keyword"
                },
                "targetElementId": {
                  "type": "keyword"
                }
              }
            },
            "tenantId": {
              "type": "keyword"
            },
            "rootProcessInstanceKey": {
              "type": "long"
            }
          }
        }
      }
    }
  }
}
