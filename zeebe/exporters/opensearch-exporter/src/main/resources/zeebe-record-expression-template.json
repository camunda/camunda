{
  "index_patterns": [
    "zeebe-record_expression_*"
  ],
  "composed_of": [
    "zeebe-record"
  ],
  "priority": 20,
  "version": 1,
  "template": {
    "settings": {
      "number_of_shards": 1,
      "number_of_replicas": 0,
      "index.queries.cache.enabled": false
    },
    "aliases": {
      "zeebe-record-expression": {}
    },
    "mappings": {
      "properties": {
        "value": {
          "dynamic": "strict",
          "properties": {
            "expression": {
              "type": "keyword",
              "ignore_above": 8191
            },
            "resultValue": {
              "type": "object",
              "enabled": false
            },
            "warnings": {
              "type": "keyword"
            },
            "tenantId": {
              "type": "keyword"
            }
          }
        }
      }
    }
  }
}

