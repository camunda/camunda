    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.24",
    "eslint-plugin-testing-library": "7.6.3",
    "globals": "16.3.0",
    "jsdom": "26.1.0",
    "monaco-editor": "0.52.2",
    "msw": "2.10.3",
