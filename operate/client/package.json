  "scripts": {
    "start": "vite",
    "build": "npm run ts-check && vite build && node ./renameProdIndex.js && node license-build.js",
    "test": "vitest run --project=unit",
    "test:browser": "vitest run --project=browser",
    "ts-check": "tsc -b",
    "eslint": "eslint .",
    "lint": "npm run ts-check && npm run eslint",
    "fix:prettier": "prettier --write \"**/*.ts\"",
    "test:e2e": "cross-env IS_E2E=true PORT=8081 ZEEBE_GATEWAY_ADDRESS=localhost:26503 ZEEBE_REST_GATEWAY_ADDRESS=http://localhost:8081 playwright test --project=e2e",
    "test:a11y": "playwright test --project=a11y",
    "test:visual": "playwright test --project=visual-light --project=visual-dark",
    "generate-screenshots": "playwright test --project=screenshot-generator",
    "test:e2e:ci": "cross-env IS_E2E=true PORT=8080 playwright test --project=e2e",
    "coverage": "npm run test -- --coverage --changed",
    "start-visual-regression-docker": "docker run --rm --network host -v $(pwd):/work/ -w /work/ -it mcr.microsoft.com/playwright:v1.54.0 /bin/bash",
    "start:visual-regression": "serve build/ -p 8081 -n -s -L",
    "build:visual-regression": "cross-env VITE_VERSION=0.0.0-SNAPSHOT vite build --mode visual-regression",
    "extract-sbom": "vite build --mode sbom"
  },
  "devDependencies": {
    "@camunda8/sdk": "8.7.30",
    "@eslint/js": "9.32.0",
    "@playwright/test": "1.54.0",
    "@testing-library/dom": "10.4.1",
