{
	"order": 30,
	"index_patterns": [
		"operate-event-${schema.version}_*"
	],
	"mappings": {
		"dynamic": "strict",
		"dynamic_templates": [
			{
				"string_template": {
					"path_match": "*",
					"mapping": {
						"norms": {
							"enabled": false
						},
						"type": "string",
						"index_options": "docs"
					},
					"match_mapping_type": "string"
				}
			}
		],
		"properties": {
			"dateTime": {
				"format": "date_time",
				"type": "date"
			},
			"metadata": {
				"properties": {
					"jobRetries": {
						"type": "integer"
					},
					"incidentErrorType": {
						"type": "keyword"
					},
					"jobCustomHeaders": {
						"enabled": false
					},
					"incidentErrorMessage": {
						"type": "text"
					},
					"jobWorker": {
						"type": "keyword"
					},
					"jobKey": {
						"type": "long"
					},
					"jobType": {
						"type": "keyword"
					},
					"jobDeadline": {
						"format": "date_time",
						"type": "date"
					}
				}
			},
			"partitionId": {
				"type": "integer"
			},
			"eventSourceType": {
				"type": "keyword"
			},
			"eventType": {
				"type": "keyword"
			},
			"flowNodeId": {
				"type": "keyword"
			},
			"flowNodeInstanceKey": {
				"type": "long"
			},
			"workflowKey": {
				"type": "long"
			},
			"workflowInstanceKey": {
				"type": "long"
			},
			"id": {
				"type": "keyword"
			},
			"bpmnProcessId": {
				"type": "keyword"
			},
			"key": {
				"type": "long"
			}
		}
	},
	"aliases": {
		"operate-event-${schema.version}_alias": {}
	}
}
