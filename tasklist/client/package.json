  "scripts": {
    "start": "vite",
    "build": "tsc && vite build && node ./renameProdIndex.mjs",
    "test": "TZ=utc vitest run --retry=3",
    "test:ci": "CI=true npm run test",
    "test:visual": "playwright test --project=visual",
    "test:a11y": "IS_A11Y=true playwright test --project=a11y",
    "test:docs": "CI=true IS_E2E=true playwright test --project=docs",
    "ts-check": "tsc && tsc -p ./e2e/tsconfig.json",
    "eslint": "eslint .",
    "stylelint": "stylelint \"src/**/*.?(s)css\"",
    "lint": "npm run ts-check && npm run eslint && npm run stylelint",
    "coverage": "npm run test run --coverage --changed",
    "analyze-bundle": "npm run build && source-map-explorer 'build/assets/*.js'",
    "start-visual-regression-docker": "docker run --rm --network host -v $(pwd):/work/ -w /work/ -it mcr.microsoft.com/playwright:v1.56.1 /bin/bash",
    "start:visual-regression": "serve build/ -p 8081 -n -s -L",
    "build:visual-regression": "VITE_VERSION=0.0.0-SNAPSHOT vite build --mode visual-regression",
    "extract-sbom": "vite build --mode sbom"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
