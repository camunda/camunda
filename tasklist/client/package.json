    "eslint": "eslint .",
    "stylelint": "stylelint \"src/**/*.?(s)css\"",
    "lint": "yarn ts-check && yarn eslint && yarn stylelint",
    "coverage": "yarn test run --coverage --changed",
    "analyze-bundle": "yarn build && source-map-explorer 'build/assets/*.js'",
