    "license-checker": "25.0.1",
    "prettier": "3.6.2",
    "rollup": "4.45.0",
    "rollup-plugin-license": "^3.5.3",
    "sass": "1.89.2",
    "typescript": "5.8.2",
    "vite": "7.0.0",
    "vite-plugin-svgr": "^4.3.0"
