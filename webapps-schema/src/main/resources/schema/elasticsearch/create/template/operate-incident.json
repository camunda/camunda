{
	"settings": {
		"analysis": {
			"analyzer": {
				"ca_path_tree": {
					"tokenizer": "ca_hierarchy"
				}
			},
			"tokenizer": {
				"ca_hierarchy": {
					"type": "path_hierarchy",
					"delimiter": "/"
				}
			}
		}
	},
	"mappings": {
    "dynamic": "strict",
		"properties": {
			"creationTime": {
				"format": "date_time || epoch_millis",
				"type": "date"
			},
			"flowNodeId": {
				"type": "keyword"
			},
			"partitionId": {
				"type": "integer"
			},
			"errorType": {
				"type": "keyword"
			},
			"flowNodeInstanceKey": {
				"type": "long"
			},
			"errorMessage": {
				"type": "text"
			},
			"errorMessageHash": {
				"type":"integer"
			},
			"processDefinitionKey": {
				"type": "long"
			},
			"jobKey": {
				"type": "long"
			},
			"processInstanceKey": {
				"type": "long"
			},
			"bpmnProcessId": {
				"type": "keyword"
			},
			"id": {
				"type": "keyword"
			},
			"state": {
				"type": "keyword"
			},
			"key": {
				"type": "long"
			},
			"treePath": {
				"type": "text",
				"analyzer": "ca_path_tree",
				"fielddata": true
			},
      "tenantId": {
        "type": "keyword"
      },
      "position": {
        "type": "long"
      },
      "fake": {
        "type": "keyword"
      }
		}
	}
}
