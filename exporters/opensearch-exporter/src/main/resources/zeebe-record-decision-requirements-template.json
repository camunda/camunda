{
  "index_patterns": [
    "zeebe-record_decision-requirements_*"
  ],
  "composed_of": ["zeebe-record"],
  "priority": 20,
  "version": 1,
  "template": {
    "settings": {
      "number_of_shards": 1,
      "number_of_replicas": 0,
      "index.queries.cache.enabled": false
    },
    "aliases": {
      "zeebe-record-decision-requirements": {}
    },
    "mappings": {
      "properties": {
        "value": {
          "dynamic": "strict",
          "properties": {
            "decisionRequirementsId": {
              "type": "keyword"
            },
            "decisionRequirementsName": {
              "type": "keyword"
            },
            "decisionRequirementsVersion": {
              "type": "long"
            },
            "decisionRequirementsKey": {
              "type": "long"
            },
            "namespace": {
              "type": "keyword"
            },
            "resourceName": {
              "type": "text"
            },
            "resource": {
              "enabled": false
            },
            "checksum": {
              "enabled": false
            },
            "duplicate": {
              "type": "boolean"
            }
          }
        }
      }
    }
  }
}
